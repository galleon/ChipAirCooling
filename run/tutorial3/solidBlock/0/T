FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
    solidWall
    {
        type            zeroGradient;
    }

    solidCoupledWall
    {
        type            chtRcTemperature;
        radiation       off;
        K               k;
        remoteField     T;
        value           uniform 273;
    }

    solidSide
    {
        type            symmetryPlane;
    }

    defaultFaces
    {
        type            empty;
    }

}

