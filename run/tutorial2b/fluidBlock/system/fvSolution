solvers
{
    p_rgh
    {
        solver           PCG;
        preconditioner   DIC;

        minIter          0;
        maxIter          1000;
        tolerance        1e-12;
        relTol           0.01;
    };

    U
    {
        solver           PBiCG;
        preconditioner   DILU;

        minIter          1;
        maxIter          1000;
        tolerance        1e-12;
        relTol           0.01;
    };

    T+T
    {
        solver           BiCG;
        preconditioner   Cholesky;

        minIter          1;
        maxIter          100;
        tolerance        1e-12;
        relTol           0.01;
    };

    G
    {
        solver           PCG;
        preconditioner   DIC;

        minIter          1;
        maxIter          1000;
        tolerance        1e-12;
        relTol           0.1;
    };

    epsilon
    {
        solver           PBiCG;
        preconditioner   DILU;

        minIter          1;
        maxIter          100;
        tolerance        1e-12;
        relTol           0.01;
    };

    k
    {
        solver           PBiCG;
        preconditioner   DILU;

        minIter          1;
        maxIter          100;
        tolerance        1e-12;
        relTol           0.01;
    };
}

