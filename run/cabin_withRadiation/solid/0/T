FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 284;

boundaryField
{
    topExtWall
    {
        type            extendedWallHeatTransfer;

        K               k;
        radiation       on;

        Tinf            250;
        hc              2;
        alpha           50;

        radiationSources
        (
        );

        value           uniform 293;
    }

    "toF.*"
    {
        type            chtRcTemperature;
        radiation       off;
        remoteField     T;
        K               k;

        value           uniform 293;
    }

    defaultFaces
    {
        type            empty;
    }
}

