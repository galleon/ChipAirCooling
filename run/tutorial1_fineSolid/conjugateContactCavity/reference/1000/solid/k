FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1000";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 10;

boundaryField
{
    topAndBottom
    {
        type            zeroGradient;
    }
    left
    {
        type            chtRcThermalDiffusivitySlave;
        remoteField     kappaEff;
        value           nonuniform List<scalar> 10(0.265014 0.271856 0.281061 0.295592 0.313414 0.339265 0.4005 0.408984 0.373189 0.342781);
    }
    right
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


