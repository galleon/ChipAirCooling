FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1000";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 10;

boundaryField
{
    topAndBottom
    {
        type            zeroGradient;
    }
    left
    {
        type            chtRcThermalDiffusivitySlave;
        remoteField     kappaEff;
        value           nonuniform List<scalar> 10(0.120904 0.128781 0.124792 0.120938 0.126959 0.138493 0.145211 0.150244 0.153604 0.144992);
    }
    right
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


